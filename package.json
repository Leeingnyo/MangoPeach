{
  "name": "mangopeach",
  "version": "1.0.0",
  "repository": "https://github.com/Leeingnyo/MangoPeach.git",
  "author": "Lee Inyong <dyd1928@naver.com>",
  "license": "MIT",
  "private": true,
  "workspaces": ["packages/*"],
  "scripts": {
    "core:start": "cd packages/mangopeach-core && yarn start",
    "core:test": "cd packages/mangopeach-core && yarn test",
    "core:lint": "cd packages/mangopeach-core && yarn lint",
    "core:db:generate": "yarn workspace mangopeach-core run db:generate",
    "core:db:migrate": "yarn workspace mangopeach-core run db:migrate",
    "ui:dev": "cd packages/mangopeach-ui && yarn dev",
    "ui:build": "cd packages/mangopeach-ui && yarn build",
    "ui:start": "cd packages/mangopeach-ui && yarn start",
    "server:dev": "cd packages/mangopeach-server && yarn start:dev",
    "server:build": "cd packages/mangopeach-server && yarn build",
    "server:start": "cd packages/mangopeach-server && yarn start"
  }
}
